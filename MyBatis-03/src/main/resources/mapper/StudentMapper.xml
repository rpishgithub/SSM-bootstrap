<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tech.rpish.mapper.StudentMapper">
    <select id="findStudentByCourseId" parameterType="Integer" resultType="tech.rpish.po.Student">
        select * from student where id in(
            select student_id from seleCourse where course_id=#{id}
            )
    </select>
</mapper>